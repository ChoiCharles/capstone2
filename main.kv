<MainWindow>
    name: "MainWindow"
    
    BoxLayout:
        orientation: "vertical"
        size: root.width, root.height
        
        Button:
            text: "Properties"
            font_size: 50
            on_release:
                app.root.current = "Properties"
                root.manager.transition.direction = "left"
                
        Button:
            text: "Demensionless\n Number"
            font_size: 50
            on_release:
                app.root.current = "Demensionless"
                root.manager.transition.direction = "left"
                
        Button:
            text: "Convection"
            font_size: 50
            on_release:
                app.root.current = "Convection"
                root.manager.transition.direction = "left"
            

<Properties>
    name: "Properties"
    
    BoxLayout:
        orientation: "vertical"
        size: root.width, root.height
        
        GridLayout:
            cols: 2
            
            Label:
                text: "Properties"
                font_size: 50
            
            Button:
                text: "Go Back"
                font_size: 50
                on_release:
                    app.root.current = "MainWindow"
                    root.manager.transition.direction = "right"
                    
            Label:
                id: T
                text: "Temperature (Celcius)"
                font_size: 50
        	
        	TextInput:
    			id: T_input
    			hint_text: "Input Temperature"
    			font_size: 50
    			
    		Label:
                id: P
                text: "Pressure [kPa]"
                font_size: 50
        	
        	TextInput:
    			id: P_input
    			hint_text: "Input Pressure"
    			font_size: 50
    			
    		Label:
                id: click_label
                text: "Fluid"
                font_size: 50
                
            Spinner:
                id: spinner_id
                text: "Choose Fluid"
                values: ["water", "air"]
                on_text: root.spinner_clicked(spinner_id.text)
                
            Label:
                id: clear
				font_size: 50
            
            Button:
                id: result
				font_size: 50
				text: "Result"
				on_press: root.result(result)
				
            Label:
                id: k
                text: "Conductivity [W/mK] ="
                font_size: 50
                on_text: root.result(k.text)
                
            Label:
                id: mu
                text: "Viscosity [m^2/s] ="
                font_size: 50
                on_text: root.result(mu.text)
                
            Label:
                id: rho
                text: "Density [kg/m^3] ="
                font_size: 50
                on_text: root.result(rho.text)
                
    		Label:
                id: cp
                text: "Specific Heat [kJ/kgK] ="
                font_size: 50
                on_text: root.result(cp.text)
                
            Label:
                id: beta
                text: "Coefficient of Expansion [1/K] ="
                font_size: 35
                on_text: root.result(beta.text)
                
            Label:
                id: enthalpy
                text: "Enthalpy [kJ/kg] ="
                font_size: 50
                on_text: root.result(enthalpy.text)


